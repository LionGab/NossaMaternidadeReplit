# Resolver conflitos de peer deps (Expo SDK 54 / libs auxiliares)
legacy-peer-deps=true

# Cache agressivo - SSD rápido aguenta
cache=~/.npm
cache-min=999999999

# Parallel downloads
maxsockets=8

# Strict SSL (segurança)
strict-ssl=true

# Save exact versions
save-exact=true

# Disable funding messages
fund=false

# Disable update notifier
update-notifier=false

# Progress bars
progress=true
loglevel=error
